{
  "name": "karet-todomvc",
  "private": true,
  "description": "Karet TodoMVC",
  "scripts": {
    "build": "cp node_modules/babel-polyfill/dist/polyfill.min.js docs/app.js && NODE_ENV=production browserify src/todomvc.js -t babelify -g envify -g uglifyify | uglifyjs -m -c >> docs/app.js",
    "lint": "eslint src test",
    "prepublish": "npm run lint && npm run build",
    "watch": "watchify src/todomvc.js -v --loadMaps --debug -t babelify -o docs/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/calmm-js/karet-todomvc.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/calmm-js/karet-todomvc/issues"
  },
  "homepage": "https://github.com/calmm-js/karet-todomvc#readme",
  "dependencies": {
    "atom.storage": "^0.5.0",
    "karet": "^1.0.0",
    "karet.util": "^0.11.1",
    "kefir": "^3.7.1",
    "kefir.atom": "^5.2.1",
    "partial.lenses": "^9.7.0",
    "ramda": "^0.23.0",
    "react-dom": "^15.4.2",
    "todomvc-app-css": "^2.0.6"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.23.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "babelify": "^7.3.0",
    "browserify": "^14.1.0",
    "envify": "^4.0.0",
    "eslint": "^3.17.1",
    "eslint-plugin-react": "^6.10.0",
    "uglify-js": "^2.8.12",
    "uglifyify": "^3.0.4",
    "watchify": "^3.9.0"
  }
}
